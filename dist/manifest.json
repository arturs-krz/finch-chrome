{
    "manifest_version": 2,
    "name": "Finch",
    "version": "0.1",
    "minimum_chrome_version": "10.0",
    "permissions": [
        "debugger", "tabs", "activeTab", "storage", "unlimitedStorage"
    ],
    "devtools_page": "finch_devtools.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
}